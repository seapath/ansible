Defaults:Centos-snmp !requiretty
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_diskusage.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_tempdisk.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_ipmitool_sensor.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_ipmitool_sensor_v.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_ipmitool_sdrlist.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_ipmitool_sdrlist_v.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_lvs.sh ""
Centos-snmp     ALL = NOPASSWD:EXEC: /usr/local/bin/snmp_smartctl.sh ""
Centos-snmp     ALL = (hacluster) NOPASSWD:EXEC: /usr/local/bin/snmp_crmstatus.sh ""
Centos-snmp     ALL = (libvirt) NOPASSWD:EXEC: /usr/local/bin/snmp_virshlist.sh ""
Centos-snmp     ALL = (libvirt) NOPASSWD:EXEC: /usr/local/bin/snmp_domstats.sh ""
Centos-snmp     ALL = (libvirt) NOPASSWD:EXEC: /usr/local/bin/snmp_dommemstat.sh ""
Centos-snmp     ALL = (ceph) NOPASSWD:EXEC: /usr/local/bin/snmp_cephstatus.sh ""
Centos-snmp     ALL = (ceph) NOPASSWD:EXEC: /usr/local/bin/snmp_cephusage.sh ""
