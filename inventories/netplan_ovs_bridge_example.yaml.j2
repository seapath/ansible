network:
  version: 2
  renderer: networkd
  openvswitch:
    protocols: [OpenFlow13, OpenFlow14, OpenFlow15]
  ethernets:
    {{ ovs_ext_interface }}: {}
  bridges:
    ovs0:
      interfaces: [{{ ovs_ext_interface }}]
      openvswitch:
        protocols: [OpenFlow10, OpenFlow11, OpenFlow12]
